{
  "name" : "Edwards TIC",
  "connection" : "tcp",
  "host": "ets3",
  "port": 3002,
  "timeout": 3000, 
  "commandParameterSeperator" : " ",
  "parameterSeperator" : ";",
  "commandTerminator" : "\r\n",
  "responseTerminator" : "\r",
  "deviceErrorStatusRegex"  : "\\*[CSV]\\d{3}\\s(\\d{1,2})",
  "errorStatusNotifier" : "*",
  "commandMap": {
    "ReadG1" : {
      "command" : "?V913",
      "description" : "Read the value and state of the Gauge 1.",
      "responseList" : ["pressure", "unit","state","alert id","priority"]
    },
    "ReadG2" : {
      "command" : "?V914",
      "description" : "Read the value and state of the Gauge 2.",
      "responseList" : ["pressure", "unit","state","alert id","priority"]
    },
    "ReadG3" : {
      "command" : "?V915 1",
      "description" : "Read the value and state of the Gauge 3.",
      "responseList" : ["pressure", "unit","state","alert id","priority"]
    },
    "G3on" : {
      "command" : "!C915 1",
      "responseList" : ["g3on","happycode"]
    }
  },
  "paramMap": {
    "happycode" : {
      "name" : "errorcode",
      "type" : "int",
      "dataFormat" : "(0)"
    },
    "errorcode" : {
      "name" : "errorcode",
      "type" : "int",
      "dataFormat" : "(\\d{1,2})"
    },
    "g3on" : {
      "name" : "g3on",
      "type" : "string",
      "dataFormat" : "\\*C915"
    },
    "pressure" : {
      "name" : "pressure",
      "type" : "double",
      "dataFormat" : "(\\d*?.\\d*?[eE][+-]\\d*)",
      "description" : "Pressure (see gauge type) or voltage 0.000 to 11.000 volts."      
    },
    "unit" : {
      "name" : "unit",
      "type" : "int",
      "dataFormat" : "(\\d{2})",
      "range" : "66,59,81",
      "description" : "Indicates which unit is used. 66 - Voltage, 59 - Pressure (Pascals), 81 - Percent."
    },
    "state" : {
      "name" : "state",
      "type" : "int",
      "dataFormat": "(\\d{1,2})",
      "range" : "[0,12]",
      "description" : "0 - Gauge Not connected\n1 - Gauge Connected\n2 - New Gauge Id\n3 - Gauge Change\n4 - Gauge In Alert\n5 - Off\n6 - Striking\n7 - Initialising\n8 - Calibrating\n9 - Zeroing\n10 - Degassing\n11 - On\n12 - Inihibited"
    },
    "alert id" : {
      "name" : "alert id",
      "type" : "int",
      "range" : "[0,47]",
      "dataFormat": "(\\d{1,2})",
      "description" : "Check manual for descriptions"
    },
    "priority" : {
      "name" : "priority",
      "type" : "int",
      "range" : "[0,3]",
      "dataFormat" : "(\\d{1})",
      "description" : "0 - OK\n1 - Warning\n2 - alarm\n3 - alarm"
    }
        
  }
}
